{
  "name": "protosolid",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "dev": "nuxt dev",
    "dev-electron": "electron ./electron",
    "build": "npm run build-oc && npm run build-app",
    "pack": "npm run build-app && cd electron && electron-builder",

    "build-oc": "cd assets/oc && docker run --rm -it -v \".\\:/src\" donalffons/opencascade.js oc.yml",
    "build-app": "npm run clean && npm run generate && mv -v .output/public/* electron/app",
    "generate": "nuxt generate",
    "clean": "rm -rf .output electron/app/*",

    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "nuxt": "^3.12.4",
    "vue": "latest"
  },
  "overrides": {
    "vue": "latest"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@pinia/nuxt": "^0.5.2",
    "@salusoft89/planegcs": "^1.1.6",
    "earcut": "^3.0.0",
    "electron": "31.3.0",
    "electron-builder": "^24.13.3",
    "jszip": "^3.10.1",
    "nanoevents": "^9.0.0",
    "nib": "^1.2.0",
    "nuxt-electron": "^0.7.0",
    "pinia": "^2.2.0",
    "pluralize": "^8.0.0",
    "pug": "^3.0.3",
    "stylus": "^0.63.0",
    "three": "^0.167.1",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
