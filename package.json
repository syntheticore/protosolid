{
  "name": "alchemy-web",
  "version": "0.1.0",
  "productName": "Alchemy",
  "description": "Parametric CAD",
  "author": "Bj√∂rn Breitgoff <syntheticore@gmail.com>",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "build": "npm run clean && webpack --mode production --env.production --env.wc",
    "build-electron": "npm run clean && webpack --mode production --env.production",
    "dev": "npm run clean && webpack-dev-server --env.development --hot --open -d",
    "dev-electron": "npm run build-electron && electron ./dist",
    "pack-electron": "npm run build-electron && electron-packager ./dist --overwrite --helperBundleId=com.alchemy.electron",
    "package-mac": "electron-packager ./dist --overwrite --platform=darwin --arch=x64 --icon=./alchemy.icns --prune=true --out=release-builds",
    "package-win": "electron-packager ./dist --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./alchemy.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Alchemy\"",
    "package-linux": "electron-packager ./dist --overwrite --asar=true --platform=linux --arch=x64 --icon=./alchemy1024x1024.png --prune=true --out=release-builds",
    "test": "cargo test && wasm-pack test --headless",
    "clean": "rimraf dist rust/pkg",
    "real-clean": "npm run clean && rimraf node_modules rust/**/target"
  },
  "build": {
    "appId": "com.alchemy.electron"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@vue/web-component-wrapper": "^1.2.0",
    "@wasm-tool/wasm-pack-plugin": "^1.3.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "electron": "^10.3.0",
    "electron-packager": "^15.2.0",
    "jszip": "^3.5.0",
    "mini-css-extract-plugin": "^1.3.4",
    "nanoevents": "^5.1.10",
    "nib": "^1.1.2",
    "pluralize": "^8.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.1.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.3",
    "three": "^0.124.0",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {}
}
